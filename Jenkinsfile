pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        echo 'This is a test message'
      }
    }

    stage('test') {
      steps {
        echo 'this is a test message'
      }
    }

    stage('deploy') {
      steps {
        echo 'this is a deploy phase'
      }
    }

    stage('Finish') {
      steps {
        echo 'This is a finish message'
      }
    }

  }
}